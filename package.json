    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "commander": "^10.0.1",
    "isomorphic-git": "^1.23.0",
    "ts-node": "^10.9.1",
    "octokit": "^2.0.14",
    "winston": "^3.9.0",
    "wrangler": "^3.0.1"
  }
