{
  "name": "node-worker-deploy",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "deploy.js",
  "bin": {
    "deploy": "NODE_OPTIONS='--no-warnings' ts-node --esm src/deploy.ts"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsc",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "deploy": "npm run build ; node --no-warnings ./dist/deploy.js deploy",
    "delete": "npm run build ; node --no-warnings ./dist/deploy.js delete"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230404.0",
    "@types/node": "^20.2.5",
